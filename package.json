{
  "name": "match3preloader",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/node/match3preloader.umd.js",
  "module": "./dist/node/match3preloader.es.js",
  "types": "./dist/node/match3preloader.d.ts",
  "exports": {
    ".": {
      "import": "./dist/node/match3preloader.es.js",
      "require": "./dist/node/match3preloader.umd.js",
      "types": "./dist/node/match3preloader.d.ts"
    },
    "./browser": {
      "import": "./dist/browser/match3preloader.es.js",
      "require": "./dist/browser/match3preloader.umd.js"
    },
    "./vue2": {
      "import": "./dist/vue2/match3preloader.es.js",
      "require": "./dist/vue2/match3preloader.umd.js",
      "types": "./dist/vue2/match3preloader.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JetFlynn/match3preloader"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:browser && npm run build:node && npm run build:vue2",
    "build:browser": "vite build --config vite.config.browser.ts",
    "build:node": "vite build --config vite.config.node.ts",
    "build:vue2": "vite build --config vite.config.vue2.ts",
    "dev": "vite",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vue2": "npm:vue@^2.7.16",
    "rimraf": "^6.0.1"
  }
}
